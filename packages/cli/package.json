{
  "name": "@hexmon_tech/cli",
  "version": "0.1.0",
  "description": "acx cli package.",
  "license": "MIT",
  "engines": {
    "node": ">=18"
  },
  "bin": {
    "acx": "./dist/bin.cjs"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "sideEffects": false,
  "dependencies": {
    "@hexmon_tech/compiler": "workspace:*",
    "@hexmon_tech/core": "workspace:*",
    "@hexmon_tech/engine-embedded": "workspace:*",
    "@hexmon_tech/policy-dsl": "workspace:*",
    "commander": "^12.1.0"
  },
  "scripts": {
    "build": "tsup",
    "lint": "eslint src --max-warnings=0",
    "test": "vitest run --passWithNoTests",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "pack:check": "node ../../scripts/pack/check-package.mjs"
  },
  "author": "Hexmon Technology",
  "keywords": [
    "access-control",
    "authorization",
    "cli",
    "policy",
    "devtools"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/acx-dev/acx.git",
    "directory": "packages/cli"
  },
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://github.com/acx-dev/acx",
  "bugs": {
    "url": "https://github.com/acx-dev/acx/issues"
  }
}
