{
  "name": "@acx/cli",
  "version": "0.0.0",
  "description": "acx cli package.",
  "license": "MIT",
  "engines": {
    "node": ">=18"
  },
  "bin": {
    "acx": "./dist/bin.cjs"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "sideEffects": false,
  "dependencies": {
    "@acx/compiler": "workspace:*",
    "@acx/core": "workspace:*",
    "@acx/engine-embedded": "workspace:*",
    "@acx/policy-dsl": "workspace:*",
    "commander": "^12.1.0"
  },
  "scripts": {
    "build": "tsup",
    "lint": "eslint src --max-warnings=0",
    "test": "vitest run --passWithNoTests"
  },
  "author": "ACX Authors",
  "keywords": [
    "access-control",
    "authorization",
    "cli",
    "policy",
    "devtools"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/acx-dev/acx.git",
    "directory": "packages/cli"
  },
  "publishConfig": {
    "access": "public"
  }
}
