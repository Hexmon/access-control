{
  "name": "@hexmon_tech/integrations-express",
  "version": "0.1.0",
  "description": "acx integrations-express package.",
  "license": "MIT",
  "engines": {
    "node": ">=18"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "sideEffects": false,
  "dependencies": {
    "@hexmon_tech/core": "workspace:*"
  },
  "scripts": {
    "build": "tsup",
    "lint": "eslint src --max-warnings=0",
    "test": "vitest run --passWithNoTests",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "pack:check": "node ../../scripts/pack/check-package.mjs"
  },
  "author": "Hexmon Technology",
  "keywords": [
    "access-control",
    "authorization",
    "express",
    "middleware",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Hexmon/access-control.git",
    "directory": "packages/integrations-express"
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "express": "^4.18.0 || ^5.0.0",
    "@types/express": "^4.17.21 || ^5.0.0"
  },
  "homepage": "https://github.com/Hexmon/access-control",
  "bugs": {
    "url": "https://github.com/Hexmon/access-control/issues"
  },
  "peerDependenciesMeta": {
    "@types/express": {
      "optional": true
    }
  }
}
